<div class="nt-qitem nt-qitem-edit nt-qitem-edit-inact">
    <div class="nt-qnum "></div>
    <a class="nt-qanchor"></a>
    <!-- NOT isImg START -->


    <div class="nt-conted-ph-cont" style="position: relative; overflow: hidden;">
        <div class="nt-ph" style="position: absolute; cursor: text; top: 15px; left: 40px; font-size: 16px; line-height: 22px;">Enter Question</div>
        <div class="redactor_box">
            <textarea style="height: 22px; display: none;"></textarea><div class="nt-qtext nt-qedit nt-delayedblur redactor_editor" contenteditable="true">
                <p>
                    <br />
                </p>
            </div>
        </div>
    </div>

    <!-- NOT isImg END -->

    <!-- isImg START -->
    <!-- isImg END -->

    <div class="nt-qhctrls">

        <button class="nt-btn-circle bt-duplicate"><i class="t-icon i-duplicate"></i></button>
        <button class="nt-btn-circle bt-delete"><i class="icon-remove"></i></button>


    </div>

    <div class="nt-qanscont nt-sortable">
        <div class="nt-qans nt-qans-edit nt-qans-hover">
            <div class="nt-qanselem">
                <input type="radio" />
            </div>
            <div class="nt-conted-ph-cont" style="position: relative; overflow: visible;">
                <div class="nt-ph" style="position: absolute; cursor: text; top: 5px; left: 10px; font-size: 14px; line-height: 18px;">Enter Answer</div>
                <div class="redactor_box">
                    <textarea style="height: 18px; display: none;"></textarea>
                    <div class="nt-qansdesc nt-qedit nt-delayedblur redactor_editor" contenteditable="true">
                        <p>
                            <br />
                        </p>
                    </div>
                </div>
            </div>
            <div class="nt-qansscore">
                <input type="text" class="nt-on-score" placeholder="Point Value" value="0" />
                <button class="nt-btn-sqr nt-not_scored">N/A</button>
                <!-- NOTE: bonus button disabled -->
                <button style="display: none" class="nt-btn-sqr nt-bonus"><i class="t-icon gift"></i></button>
            </div>
            <div class="nt-qansctrls">
                <button class="nt-btn-circle bt-delete" style="display: none;">
                    <i class="icon-remove"></i>
                </button>
            </div>

        </div>
        <div class="nt-qans nt-qans-edit nt-qans-hover">
            <div class="nt-qanselem">
                <input type="radio" />
            </div>
            <div class="nt-conted-ph-cont" style="position: relative; overflow: visible;">
                <div class="nt-ph" style="position: absolute; cursor: text; top: 5px; left: 10px; font-size: 14px; line-height: 18px;">Enter Answer</div>
                <div class="redactor_box">
                    <textarea style="height: 18px; display: none;"></textarea><div class="nt-qansdesc nt-qedit nt-delayedblur redactor_editor" contenteditable="true">
                        <p>
                            <br />
                        </p>
                    </div>
                </div>
            </div>
            <div class="nt-qansscore">
                <input type="text" class="nt-on-score" placeholder="Point Value" value="0" />
                <button class="nt-btn-sqr nt-not_scored">N/A</button>
                <!-- NOTE: bonus button disabled -->
                <button style="display: none" class="nt-btn-sqr nt-bonus"><i class="t-icon gift"></i></button>
            </div>
            <div class="nt-qansctrls">
                <button class="nt-btn-circle bt-delete" style="display: none;">
                    <i class="icon-remove"></i>
                </button>
            </div>

        </div>

    </div>


    <div class="nt-qfoot">

        <div class="nt-qctrls" style="display: none">

            <button class="nt-btn-text nt-qansadd" style="display: none">
                Add New Answer
            </button>


            <select class="nt-qtype-sel-predef">

                <option value="radio" selected="">Multiple choice</option>
                <option value="checkbox">Checkbox</option>
                <option value="essay">Essay</option>
                <option value="oneliner">Short answer</option>
                <option value="text">Text</option>
                <option value="img">Image</option>

            </select>
            <button class="nt-btn-sqr nt-reqbtn"><i class="t-icon asterisk"></i></button>

            <button class="nt-btn-text nt-scrbtn">
                Points
            </button>
        </div>
        <div class="nt-qtagcont">
            <div class="hoverscroll horizontal" style="width: 100%; height: 24px;">
                <div class="listcontainer" style="width: 100%; height: 24px;">
                    <div class="nt-tags list" style="width: 104px;">
                        <div class="nt-tag-adder item">
                            <input type="text" class="input-small ac_input" placeholder="Add Tag..." autocomplete="off" />
                        </div>
                    </div>
                    <div class="arrow left" style="display: none;"><i class="icon-chevron-left"></i></div>
                    <div class="arrow right" style="display: none;"><i class="icon-chevron-right"></i></div>
                </div>
            </div>
        </div>


        <button class="nt-btn-sqr nt-qctrls-qtags-toggle" style="display: none">
            <i class="t-icon i-setting"></i>
        </button>
    </div>

</div>
