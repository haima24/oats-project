@model OATS_Capstone.Models.AbsResponseQuestion
@switch (Model.Type)
{
    case "Radio":
    @Html.Partial("P_Response_Type_Radio", Model)
        break;
    case "Multiple":
    @Html.Partial("P_Response_Type_Multiple", Model)
        break;
    case "Essay":
    @Html.Partial("P_Response_Type_Essay", Model)
        break;
    case "ShortAnswer":
    @Html.Partial("P_Response_Type_ShortAnswer", Model)
        break;
    case "Text":
    @Html.Partial("P_Response_Type_Text", Model)
        break;
    case "Image":
    @Html.Partial("P_Response_Type_Image", Model)
        break;
    default:
        break;
}